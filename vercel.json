{
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/modern/(.*)", "destination": "/$1" },
    { "source": "/legacy/(.*)", "destination": "/legacy/$1" },
    { "source": "/", "destination": "/index.html" }
  ],
  "cleanUrls": true
}
